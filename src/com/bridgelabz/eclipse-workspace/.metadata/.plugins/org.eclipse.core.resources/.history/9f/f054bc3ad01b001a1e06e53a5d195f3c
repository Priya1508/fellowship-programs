package com.bridgelabz.data_structures.data_structs;

import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

public class PrimeAnagram2D {

	public static void main(String[] args) {
		List<List<Integer>> totalprime=new ArrayList<List<Integer>>();
		List<Integer>primeNumber=new ArrayList<Integer>();
		Set<Integer>primeAnagram=new HashSet<Integer>();
		List<Integer>list=new ArrayList<>();
		System.out.println("Prime number between 0 to 1000:");
		primeNumber=Utility.primeNumbers(0,1000);
		System.out.println(primeNumber);
		
		primeAnagram=Utility.primeAnagram(primeNumber);
		System.out.println("Size of the prime number which are anagram is "+primeNumber.size()+" and the number are: ");
		System.out.println((primeAnagram));
		
		list.addAll(primeAnagram);
		for(int i=0;i<list.size();i++)
		{
			if(primeNumber.contains(list.get(i)))
			{
				primeNumber.remove(list.get(i));
			}
		}
		System.out.println("Total prime number which are not anagram are "+primeNumber.size()+" and the numbers are: ");
		System.out.println(primeNumber);
		totalprime.add(primeNumber);
	}

}
